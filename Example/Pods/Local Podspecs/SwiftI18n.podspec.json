{
  "name": "SwiftI18n",
  "version": "1.5.0",
  "summary": "I18n library for Swift",
  "homepage": "https://github.com/infinum/ios-swiftI18n",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Vlaho Poluta": "vlaho.poluta@infinum.com"
  },
  "source": {
    "git": "https://github.com/infinum/ios-swiftI18n.git",
    "tag": "1.5.0"
  },
  "description": "If you are not using Apple's i18n and need to provide language picking in settings in your application, this is the place for you.",
  "requires_arc": true,
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "default_subspecs": "I18n",
  "subspecs": [
    {
      "name": "I18n",
      "source_files": [
        "SwiftI18n/Classes/Main/**/*",
        "SwiftI18n/Classes/Views/{BaseViews,PlainViews,SwiftUI}/**/*"
      ],
      "resource_bundles": {
        "SwiftI18n": [
          "SwiftI18n/SupportingFiles/PrivacyInfo.xcprivacy"
        ]
      }
    },
    {
      "name": "I18n+Case",
      "source_files": [
        "SwiftI18n/Classes/{Main,Case}/**/*",
        "SwiftI18n/Classes/Views/{BaseViews,CaseViews,SwiftUI}/**/*"
      ],
      "resource_bundles": {
        "SwiftI18n": [
          "SwiftI18n/SupportingFiles/PrivacyInfo.xcprivacy"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
